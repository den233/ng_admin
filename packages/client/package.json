{
    "name": "typerx-client",
    "version": "0.2.0",
    "description": "ng-zorro-antd admin panel front-end framework",
    "homepage": "https://vellengs.github.io/typerx/",
    "bugs": {
        "url": "https://github.com/vellengs/typerx/issues"
    },
    "license": "MIT",
    "author": "vellengs",
    "scripts": {
        "clean": "rimraf ./../server/client",
        "analyze": "ng build --prod --build-optimizer --stats-json",
        "build": "npm run clean && ng build --prod --build-optimizer && npm run build-copy",
        "precommit": "npm run lint-staged",
        "e2e": "ng e2e",
        "lint": "run-s lint:ts lint:style",
        "lint-staged": "lint-staged",
        "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
        "lint:ts": "ng lint",
        "ng": "ng",
        "serve:hmr": "ng serve -o --hmr -e=hmr",
        "build:config": "tsc -p tsconfig.appearances.json",
        "start": "ng serve -o --port 4700 --proxy-config proxy.conf.json",
        "test": "ng test -sr -sm=false",
        "test-coverage": "ng test -sr -cc -sm=false",
        "build-copy": "ts-node build.copy.ts"
    },
    "dependencies": {
        "@angular/animations": "^6.0.0",
        "@angular/common": "^6.0.0",
        "@angular/compiler": "^6.0.0",
        "@angular/core": "^6.0.0",
        "@angular/forms": "^6.0.0",
        "@angular/http": "^6.0.0",
        "@angular/platform-browser": "^6.0.0",
        "@angular/platform-browser-dynamic": "^6.0.0",
        "@angular/router": "^6.0.0",
        "@antv/data-set": "^0.8.8",
        "@antv/g2": "^3.0.11",
        "@antv/g2-plugin-slider": "^2.0.2",
        "@delon/abc": "^1.1.1",
        "@delon/acl": "^1.1.1",
        "@delon/auth": "^1.1.1",
        "@delon/cache": "^1.1.1",
        "@delon/form": "^1.1.1",
        "@delon/mock": "^1.1.1",
        "@delon/theme": "^1.1.1",
        "@delon/util": "^1.1.1",
        "@ngx-translate/core": "^10.0.1",
        "@ngx-translate/http-loader": "^3.0.1",
        "ajv": "^6.4.0",
        "array-to-tree": "^2.2.1",
        "core-js": "^2.5.4",
        "file-saver": "^1.3.8",
        "ng-zorro-antd": "^1.1.0",
        "ngx-countdown": "^3.0.0",
        "ngx-tinymce": "^2.0.0",
        "ngx-ueditor": "^2.1.0",
        "ngx-umeditor": "^2.0.0",
        "qrious": "^4.0.2",
        "rxjs": "^6.0.0",
        "rxjs-compat": "^6.2.1",
        "screenfull": "^3.3.1",
        "simple-line-icons": "^2.4.1",
        "systemjs": "^0.21.3",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.1",
        "@angular/cli": "~6.0.0",
        "@angular/compiler-cli": "^6.0.0",
        "@angular/language-service": "^6.0.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/jszip": "^3.1.3",
        "@types/lodash": "^4.14.109",
        "@types/mockjs": "^1.0.0",
        "@types/node": "~8.9.4",
        "@types/shelljs": "^0.8.0",
        "@types/systemjs": "^0.20.6",
        "codecov": "^3.0.2",
        "codelyzer": "~4.2.1",
        "editorconfig-tools": "^0.1.1",
        "gh-pages": "^1.1.0",
        "husky": "^0.14.3",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "less": "~2.7.0",
        "lint-staged": "^7.1.0",
        "mockjs": "^1.0.1-beta3",
        "prettier": "^1.12.1",
        "prettier-stylelint": "^0.4.2",
        "protractor": "~5.3.0",
        "shelljs": "^0.8.2",
        "stylelint": "^9.2.0",
        "stylelint-config-standard": "^18.2.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "tslint-config-prettier": "^1.12.0",
        "tslint-language-service": "^0.9.9",
        "typescript": "~2.7.2",
        "webpack-bundle-analyzer": "^2.11.2",
        "xlsx": "^0.12.12"
    },
    "engines": {
        "node": ">= 6.9.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "lint-staged": {
        "*.{cmd,html,json,md,sh,txt,xml,yml}": [
            "editorconfig-tools fix",
            "git add"
        ]
    }
}
